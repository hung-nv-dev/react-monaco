{
  "name": "soc-query-editor",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/soc-query-editor.cjs",
  "module": "./dist/soc-query-editor.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/soc-query-editor.mjs",
      "require": "./dist/soc-query-editor.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run build:types",
    "build:lib": "vite build && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist -p tsconfig.lib.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "antd": "^5.0.0",
    "monaco-editor": "^0.55.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@storybook/addon-a11y": "^10.1.6",
    "@storybook/addon-docs": "^10.1.6",
    "@storybook/react-vite": "^10.1.6",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/styled-components": "^5.1.36",
    "@vitejs/plugin-react": "^4.3.1",
    "antd": "^5.28.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "monaco-editor": "^0.55.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "storybook": "^10.1.6",
    "styled-components": "^6.1.19",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^5.4.11"
  }
}
